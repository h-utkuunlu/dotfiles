# Aliases
alias open="xdg-open"
alias sn="shutdown now"
alias rm='echo "This is not the command you are looking for. Use trash-cli, or /bin/rm"; false'
alias nyuadvpn="sudo openconnect --authgroup nyu-vpn vpn.abudhabi.nyu.edu"
